# Server
PORT=3000
NODE_ENV=development

# Access Control
ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000
API_KEYS=ftm_your_api_key_here

# URL Encryption (32 char hex = 16 bytes for AES-128)
# Generate with: python -c "import os; print(os.urandom(16).hex())"
URL_ENCRYPT_KEY=

# Network
REQUEST_TIMEOUT=30000

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=60000

# Python Extractor (local dev only)
PYTHON_SERVER_PORT=3001
FLASK_DEBUG=false
